{
  "name": "commercetools/raml-php-sdk",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Commercetools\\": [
        "lib/commercetools-base/src/"
      ],
      "Commercetools\\Api\\": [
        "lib/commercetools-api/src/"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Commercetools\\Test\\": [
        "lib/commercetools-base/test/unit/"
      ],
      "Commercetools\\Api\\Test\\": [
        "lib/commercetools-base/test/unit/"
      ]
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "guzzlehttp/psr7": "^1.1",
    "guzzlehttp/guzzle": "^6.0",
    "psr/cache": "^1.0",
    "psr/simple-cache": "^1.0",
    "psr/log": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "cache/filesystem-adapter": "^1.0"
  },
  "require-dev": {
    "monolog/monolog": "^1.3",
    "phpunit/phpunit": "^8.0",
    "vimeo/psalm": "^3.4",
    "cache/array-adapter": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  }
}
